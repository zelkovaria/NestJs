const dexter = {
    name : 'Dexter Han',
    age : 21,
    hobby : ['movie', 'Billiards'],
}


interface User {
    name : string;
    age: number;
}

const user: User ={
    name: 'Dexter',
    age: 21,
}

class User {
    constructor(name: string, age: number){}
}

const user: User = new User('Dexter', 21);

type MyUser = User;

function getLength(obj : string| string[]){
    return obj.length;
}

type Status = "Ready" | "Waiting";

function identity(arg: any): any {
    return arg;
}